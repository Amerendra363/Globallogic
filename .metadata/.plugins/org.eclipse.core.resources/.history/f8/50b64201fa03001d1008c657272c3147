package com.globallogic.helloworld;

class A {
	public void printA() {
		System.out.println("A");

	}

}

class B extends A{
	public void printB() {
		System.out.println("B");
		
	}

}

class c {

}

public class MyInheritance {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A a=new A();
		a.printA();
		B b=new B();
		b.printB();
		b.printA();
		
		
		

	}

}
